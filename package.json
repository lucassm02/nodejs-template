    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "elastic-apm-node": "^4.5.4",
    "fastify": "^4.27.0",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.2.0",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "memjs": "^1.3.2",
    "mongodb": "^6.6.1",
    "mongoose": "^8.3.4",
    "node-schedule": "^2.1.1",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "string-template": "^1.0.0",
    "tedious": "18.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
